<div class="top">
    <div class="header">
        <div class="header_icon">
            <h1 class="icon">Daita</h1>
        </div>
        <div class="devise">
            <div class="select-boxes">
                  <%= link_to "TOP", root_path, class: "select-box" %>
                  <%= link_to current_user.nickname, '#', class: "select-box" %>
            </div>
        </div>
    </div>
    <div class="main">
        <div class="agenda_box">
            <div class="agenda_title_show">
                タイトル：　<%= @agenda.title %>
            </div>
            <div class="agenda_overview_show">
                概要：<br><%= @agenda.catch_copy %>
            </div>
        </div>
    </div>
</div>

